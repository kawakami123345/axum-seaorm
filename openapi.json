    "/publishers/{pub_id}": {
      "get": {
        "tags": [
          "Publisher"
        ],
        "operationId": "get_publisher",
        "parameters": [
          {
            "name": "pub_id",
            "in": "path",
            "description": "Publisher pub_id",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Get publisher by pub_id",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublisherResponseDto"
                }
              }
            }
          },
          "404": {
            "description": "Publisher not found"
          }
        }
      },
      "put": {
        "tags": [
          "Publisher"
        ],
        "operationId": "update_publisher",
        "parameters": [
          {
            "name": "pub_id",
            "in": "path",
            "description": "Publisher pub_id",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PublisherUpdateDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Publisher updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PublisherResponseDto"
                }
              }
            }
          },
          "404": {
            "description": "Publisher not found"
          }
        }
      },
      "delete": {
        "tags": [
          "Publisher"
        ],
        "operationId": "delete_publisher",
        "parameters": [
          {
            "name": "pub_id",
            "in": "path",
            "description": "Publisher pub_id",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Publisher deleted successfully"
          },
          "404": {
            "description": "Publisher not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BookCreateDto": {
        "type": "object",
        "required": [
          "title",
          "author",
          "publisher_id",
          "status",
          "price"
        ],
        "properties": {
          "author": {
            "type": "string"
          },
          "price": {
            "type": "integer",
            "format": "int32"
          },
          "publisher_id": {
            "type": "string",
            "format": "uuid"
          },
          "status": {
            "type": "string",
            "example": "Unapplied"
          },
          "title": {
            "type": "string"
          }
        }
      },
      "BookPublisherDto": {
        "type": "object",
        "required": [
          "pub_id",
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "pub_id": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "BookResponseDto": {
        "type": "object",
        "required": [
          "pub_id",
          "title",
          "author",
          "publisher",
          "status",
          "price"
        ],
        "properties": {
          "author": {
            "type": "string"
          },
          "price": {
            "type": "integer",
            "format": "int32"
          },
          "pub_id": {
            "type": "string",
            "format": "uuid"
          },
          "publisher": {
            "$ref": "#/components/schemas/BookPublisherDto"
          },
          "status": {
            "type": "string",
            "example": "Unapplied"
          },
          "title": {
            "type": "string"
          }
        }
      },
      "BookUpdateDto": {
        "type": "object",
        "required": [
          "pub_id",
          "title",
          "author",
          "publisher_id",
          "status",
          "price"
        ],
        "properties": {
          "author": {
            "type": "string"
          },
          "price": {
            "type": "integer",
            "format": "int32"
          },
          "pub_id": {
            "type": "string",
            "format": "uuid"
          },
          "publisher_id": {
            "type": "string",
            "format": "uuid"
          },
          "status": {
            "type": "string",
            "example": "Unapplied"
          },
          "title": {
            "type": "string"
          }
        }
      },
      "PublisherCreateDto": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          }
        }
      },
      "PublisherResponseDto": {
        "type": "object",
        "required": [
          "pub_id",
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "pub_id": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "PublisherUpdateDto": {
        "type": "object",
        "required": [
          "pub_id",
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "pub_id": {
            "type": "string",
            "format": "uuid"
          }
        }
      }
    }
  }
}